    <pd:activity name="Generate Error">
        <pd:type>com.tibco.pe.core.GenerateErrorActivity</pd:type>
        <pd:resourceType>ae.activities.throw</pd:resourceType>
        <pd:x>78</pd:x>
        <pd:y>192</pd:y>
        <config>
            <faultName>Schema</faultName>
        </config>
        <pd:inputBindings>
            <ns2:ActivityInput>
                <xsl:choose>
                    <xsl:when test="string-length($Start/pfx:Login/pfx:Username)=0">
                        <data>
                            <pfx4:Root>
                                <pfx4:Message>
                                    <xsl:value-of select="$_globalVariables/ns3:GlobalVariables/Validations/Error_Messages/User_Login/Username_Empty"/>
                                </pfx4:Message>
                                <pfx4:Message_Code>
                                    <xsl:value-of select="'100'"/>
                                </pfx4:Message_Code>
                            </pfx4:Root>
                        </data>
                    </xsl:when>
                    <xsl:when test="not(CustomFuction:matchRegex($Start/pfx:Login/pfx:Username,'[A-Za-z][A-Za-z0-9_]{1,9}'))">
                        <data>
                            <pfx4:Root>
                                <pfx4:Message>
                                    <xsl:value-of select="$_globalVariables/ns3:GlobalVariables/Validations/Error_Messages/User_Login/Username_Invalid"/>
                                </pfx4:Message>
                                <pfx4:Message_Code>
                                    <xsl:value-of select="'101'"/>
                                </pfx4:Message_Code>
                            </pfx4:Root>
                        </data>
                    </xsl:when>
                    <xsl:when test="string-length($Start/pfx:Login/pfx:Password)=0">
                        <data>
                            <pfx4:Root>
                                <pfx4:Message>
                                    <xsl:value-of select="$_globalVariables/ns3:GlobalVariables/Validations/Error_Messages/User_Login/Password_empty"/>
                                </pfx4:Message>
                                <pfx4:Message_Code>
                                    <xsl:value-of select="'102'"/>
                                </pfx4:Message_Code>
                            </pfx4:Root>
                        </data>
                    </xsl:when>
                    <xsl:when test="not(CustomFuction:matchRegex($Start/pfx:Login/pfx:Password,'[A-Za-z0-9]{1,8}'))">
                        <data>
                            <pfx4:Root>
                                <pfx4:Message>
                                    <xsl:value-of select="$_globalVariables/ns3:GlobalVariables/Validations/Error_Messages/User_Login/Password_Invalid"/>
                                </pfx4:Message>
                                <pfx4:Message_Code>
                                    <xsl:value-of select="'103'"/>
                                </pfx4:Message_Code>
                            </pfx4:Root>
                        </data>
                    </xsl:when>
                    <xsl:otherwise>
                        <data>
                            <pfx4:Root>
                                <pfx4:Message>
                                    <xsl:value-of select="'Input invalid'"/>
                                </pfx4:Message>
                                <pfx4:Message_Code>
                                    <xsl:value-of select="'201'"/>
                                </pfx4:Message_Code>
                            </pfx4:Root>
                        </data>
                    </xsl:otherwise>
                </xsl:choose>
            </ns2:ActivityInput>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Login">
        <pd:type>com.tibco.plugin.jdbc.JDBCQueryActivity</pd:type>
        <pd:resourceType>ae.activities.JDBCQueryActivity</pd:resourceType>
        <pd:x>447</pd:x>
        <pd:y>81</pd:y>
        <config>
            <timeout>10</timeout>
            <commit>false</commit>
            <maxRows>100</maxRows>
            <emptyStrAsNil>false</emptyStrAsNil>
            <jdbcSharedConfig>/Connections/JDBC Connection.sharedjdbc</jdbcSharedConfig>
            <statement>Select USERNAME, PASSWORD
FROM AIRLINE_LOGIN
WHERE USERNAME=? AND PASSWORD=?</statement>
            <Prepared_Param_DataType>
                <parameter>
                    <parameterName>USERNAME</parameterName>
                    <dataType>VARCHAR</dataType>
                </parameter>
                <parameter>
                    <parameterName>PASSWORD</parameterName>
                    <dataType>VARCHAR</dataType>
                </parameter>
            </Prepared_Param_DataType>
            <QueryOutputCachedSchemaColumns>USERNAME</QueryOutputCachedSchemaColumns>
            <QueryOutputCachedSchemaDataTypes>12</QueryOutputCachedSchemaDataTypes>
            <QueryOutputCachedSchemaDataTypesName>VARCHAR2</QueryOutputCachedSchemaDataTypesName>
            <QueryOutputCachedSchemaStatus>OptionalElement</QueryOutputCachedSchemaStatus>
            <QueryOutputCachedSchemaColumns>PASSWORD</QueryOutputCachedSchemaColumns>
            <QueryOutputCachedSchemaDataTypes>12</QueryOutputCachedSchemaDataTypes>
            <QueryOutputCachedSchemaDataTypesName>VARCHAR2</QueryOutputCachedSchemaDataTypesName>
            <QueryOutputCachedSchemaStatus>OptionalElement</QueryOutputCachedSchemaStatus>
            <oraObjects/>
            <oraTables/>
        </config>
        <pd:inputBindings>
            <jdbcQueryActivityInput>
                <USERNAME>
                    <xsl:choose>
                        <xsl:when test="exists($Start/pfx:Login/pfx:Username)">
                            <xsl:value-of select="$Start/pfx:Login/pfx:Username"/>
                        </xsl:when>
                        <xsl:otherwise>
                            <xsl:attribute name="xsi:nil">true</xsl:attribute>
                        </xsl:otherwise>
                    </xsl:choose>
                </USERNAME>
                <PASSWORD>
                    <xsl:choose>
                        <xsl:when test="exists($Start/pfx:Login/pfx:Password)">
                            <xsl:value-of select="$Start/pfx:Login/pfx:Password"/>
                        </xsl:when>
                        <xsl:otherwise>
                            <xsl:attribute name="xsi:nil">true</xsl:attribute>
                        </xsl:otherwise>
                    </xsl:choose>
                </PASSWORD>
            </jdbcQueryActivityInput>
        </pd:inputBindings>
    </pd:activity>
