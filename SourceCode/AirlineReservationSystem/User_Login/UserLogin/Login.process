    <pd:startType ref="pfx:Login"/>
    <pd:startX>75</pd:startX>
    <pd:startY>81</pd:startY>
    <pd:returnCoercions>
        <pd:coercion xpath="$_error/ns4:ErrorReport/Data/*" type="pfx4:RootType"/>
    </pd:returnCoercions>
    <pd:returnBindings>
        <xsl:choose>
            <xsl:when test="exists($_error/ns4:ErrorReport)">
                <pfx2:Message>
                    <xsl:value-of select="$_error/ns4:ErrorReport/Msg"/>
                </pfx2:Message>
            </xsl:when>
            <xsl:when test="contains($_error/ns4:ErrorReport/Data/*/pfx4:Message_Code,'100') or&#xA;contains($_error/ns4:ErrorReport/Data/*/pfx4:Message_Code,'101') or &#xA;contains($_error/ns4:ErrorReport/Data/*/pfx4:Message_Code,'102') or &#xA;contains($_error/ns4:ErrorReport/Data/*/pfx4:Message_Code,'103') or&#xA;contains($_error/ns4:ErrorReport/Data/*/pfx4:Message_Code,'201')">
                <pfx2:Message>
                    <xsl:value-of select="$_error/ns4:ErrorReport/Data/*/pfx4:Message"/>
                </pfx2:Message>
            </xsl:when>
            <xsl:when test="exists($Login/resultSet/Record)">
                <pfx2:Message>
                    <xsl:value-of select="'login Success'"/>
                </pfx2:Message>
            </xsl:when>
            <xsl:otherwise>
                <pfx2:Message>
                    <xsl:value-of select="'Invalid Input'"/>
                </pfx2:Message>
            </xsl:otherwise>
        </xsl:choose>
    </pd:returnBindings>
    <pd:endName>End</pd:endName>
    <pd:endType ref="pfx2:Message"/>
    <pd:endX>699</pd:endX>
