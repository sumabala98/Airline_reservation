                            </xsl:when>
                            <xsl:otherwise>
                                <xsl:attribute name="xsi:nil">true</xsl:attribute>
                            </xsl:otherwise>
                        </xsl:choose>
                    </ARRIVALTIME>
                    <DURATION>
                        <xsl:value-of select="pfx6:Duration"/>
