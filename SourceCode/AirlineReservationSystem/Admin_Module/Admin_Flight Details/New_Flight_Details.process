            <statement>Insert into AIRLINE_FLIGHTDETAILS ( AIRLINE,FLIGHTNUMBER,DEPARTURE,ARRIVAL,DEPARTUREDATE,ARRIVALDATE,DEPARTURETIME,ARRIVALTIME,DURATION,PRICE,STATUS)
VALUES( ?,?,?,?,TO_DATE(?, 'YYYY-MM-DD'),TO_DATE(?, 'YYYY-MM-DD'),TO_TIMESTAMP(?, 'YYYY-MM-DD HH24:MI:SS'),TO_TIMESTAMP(?, 'YYYY-MM-DD HH24:MI:SS'),?,?,?)
</statement>
